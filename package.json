{
  "name": "siege-clan-tracker",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "npm start --prefix client",
    "setup-db": "node server/scripts/setup-db.js",
    "import-csv": "node server/scripts/import-csv.js",
    "verify": "node server/scripts/verify-imports.js",
    "rebuild-db": "sqlite3 server/db/siege.db < server/db/schema.sql",
    "reset-db": "npm run rebuild-db && npm run import-csv",
    "netlify-dev": "cd client && npm run build && cd .. && netlify dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atayl16/siege-clan-tracker.git"
  },
